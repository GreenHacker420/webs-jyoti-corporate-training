{
  "name": "webs-jyoti-corporate-training",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "author": "GreenHacker",
  "description": "Professional corporate training platform for data analytics, Excel, Power BI, and business intelligence training",
  "keywords": [
    "corporate training",
    "data analytics",
    "excel training",
    "power bi",
    "business intelligence",
    "webs jyoti"
  ],
  "scripts": {
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:production": "cross-env NODE_ENV=production next build",
    "dev": "cross-env NODE_ENV=development node server.js",
    "dev:next": "next dev -p 3000",
    "dev:alt": "next dev -p 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "start": "cross-env NODE_ENV=production node server.js",
    "start:next": "next start",
    "start:dev": "cross-env NODE_ENV=development node server.js",
    "start:production": "cross-env NODE_ENV=production node server.js",
    "deploy:build": "npm run build:production && npm run deploy:prepare",
    "deploy:prepare": "node scripts/prepare-deployment.js",
    "deploy:test": "npm run build && npm run start:production",
    "deploy:validate": "node scripts/validate-deployment.js",
    "deploy:package": "node scripts/deploy.js",
    "deploy:full": "npm run deploy:validate && node scripts/deploy.js",
    "clean": "rimraf .next out node_modules/.cache",
    "clean:all": "rimraf .next out node_modules",
    "postinstall": "npm run build:production",
    "health-check": "curl -f http://localhost:3000/health || exit 1"
  },
